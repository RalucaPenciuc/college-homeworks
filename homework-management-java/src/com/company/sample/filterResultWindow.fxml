<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="304.0" prefWidth="338.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.sample.filterResultWindowController">
   <children>
      <TableView fx:id="filterTable" prefHeight="281.0" prefWidth="337.0">
        <columns>
          <TableColumn fx:id="idStudentColumn" prefWidth="75.0" text="Id Student" />
          <TableColumn fx:id="idTemaColumn" prefWidth="78.0" text="Id Tema" />
            <TableColumn fx:id="notaColumn" prefWidth="87.0" text="Nota" />
            <TableColumn fx:id="saptPredColumn" prefWidth="96.0" text="Sapt Pred" />
        </columns>
      </TableView>
      <Button fx:id="loadDataButton" layoutY="281.0" mnemonicParsing="false" onAction="#loadData" text="Load" />
      <Button fx:id="closeButton" layoutX="42.0" layoutY="281.0" mnemonicParsing="false" onAction="#close" text="Close" />
   </children>
</AnchorPane>
